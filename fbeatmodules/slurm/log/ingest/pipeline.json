{
  "description": "Filebeat Module Pipeline for parsing slurm logs (J.Strelka),
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
	"%{TIMESTAMP_ISO8601:slurm.job.endtime}] %{GREEDYDATA:slurm.job.process} JobId=%{GREEDYDATA:slurm.job.id} WEXITSTATUS %{INT:slurm.job.exit.code}",
	"%{TIMESTAMP_ISO8601:slurm.job.endtime}] %{GREEDYDATA:slurm.job.process} JobId=%{GREEDYDATA:slurm.job.id} %{WORD} %{INT:slurm.system.exit.code}",
        "%{TIMESTAMP_ISO8601:slurm.job.endtime}] %{GREEDYDATA:slurm.job.process} Allocate JobId=%{GREEDYDATA:slurm.job.id} NodeList=%{GREEDYDATA:slurm.job.nodelist} #CPUs=%{NUMBER:slurm.job.cpu.count} Partition=%{WORD:slurm.job.partition}",        "%{TIMESTAMP_ISO8601:slurm.job.starttime}] %{GREEDYDATA:slurm.job.process} Configuration for JobId=%{GREEDYDATA:slurm.job.id} is complete"
		],
 	        "ignore_missing": true
	     }
}]
}
